{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "env": {
        "production": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel",
                "babel-plugin-styled-components"
            ]
        },
        "development": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel",
                "babel-plugin-styled-components"
            ]
        },
        "test": {
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "styled-jsx/babel-test",
                "babel-plugin-styled-components"
            ]
        }
    }
}
